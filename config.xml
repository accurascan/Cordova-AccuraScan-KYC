<?xml version='1.0' encoding='utf-8'?>
<widget id="com.accurascan.ios.nfctry" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Cordova_Accurascan_Kyc</name>
    <description>Sample Apache Cordova App</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />

    <platform name="android">
        <framework custom="true" src="7.3.3" type="gradleReference" />
        <preference name="android-targetSdkVersion"  value="34" />
        <preference name="android-compileSdkVersion" value="34" />
        <preference name="android-minSdkVersion" value="21" />
        <preference name="AndroidPersistentFileLocation" value="Compatibility" />
        <allow-intent href="market:*" />
        <!-- <preference name="android-targetSdkVersion" value="22" /> -->
        <preference name="AndroidXEnabled" value="true" />
        <preference name="AndroidPersistentFileLocation" value="Compatibility" />
        <plugin name="File" value="org.apache.cordova.FileUtils" />
        <plugin name="FileTransfer" value="org.apache.cordova.FileTransfer" />
    </platform>
    <platform name="ios">
        <preference name="deployment-target" value="12.0" />
        <!-- Add other platform-specific preferences here -->
    </platform>
</widget>
